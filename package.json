{
  "name": "trpfrog.net",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint-fix": "turbo lint-fix && prettier . --write",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "reinstall-all-deps": "rm -rf node_modules ./*/*/node_modules && pnpm install",
    "prepare": "lefthook install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.4",
    "@evilmartians/lefthook": "^1.7.11",
    "@trpfrog.net/config-eslint": "workspace:*",
    "@types/node": "^20.14.12",
    "@vitejs/plugin-react": "^4.3.1",
    "better-typescript-lib": "^2.8.0",
    "eslint": "^9.10.0",
    "happy-dom": "^14.12.3",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prettier-plugin-classnames": "^0.7.3",
    "prettier-plugin-merge": "^0.7.1",
    "turbo": "^2.1.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "packageManager": "pnpm@9.1.4"
}
