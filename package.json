{
  "name": "trpfrog.net",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint-fix": "turbo lint-fix && prettier . --write",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "reinstall-all-deps": "rm -rf node_modules ./*/*/node_modules && pnpm install",
    "prepare": "lefthook install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.4.12",
    "@evilmartians/lefthook": "^1.6.22",
    "@trpfrog.net/config-eslint": "workspace:*",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-react": "^4.2.1",
    "better-typescript-lib": "^2.7.0",
    "happy-dom": "^13.5.3",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "turbo": "^2.0.6",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.1.4",
  "dependencies": {
    "eslint": "^9.6.0"
  }
}